    <rdfs:domain rdf:resource="#Channel"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasMedicationCaffeine">
    <rdfs:comment xml:lang="en">Hours since last caffeine intake, if less than 12 hours.</rdfs:comment>
