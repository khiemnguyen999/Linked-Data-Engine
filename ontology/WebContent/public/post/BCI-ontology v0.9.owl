  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasIdentity">
    <rdfs:comment xml:lang="en">The [ identity ] of the "Access Method".</rdfs:comment>
    <rdfs:domain rdf:resource="#AccessMethod"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasLocation.X">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment xml:lang="en">@XDF:
    
      [ Measured location (note: this may be arbitrary but should then include 
      well-known fiducials (landmarks) for co-registration) ]
    
    Coordinate axis pointing from the center of the head to the right, in millimeters.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Channel"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/sex">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSessionGroup">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Type of subject group the subject belongs to (e.g. autistic, normal, control...)</rdfs:comment>
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >CONTROL</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >NORMAL</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >AUTISTIC</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="isBciSessionOf">
    <rdfs:domain rdf:resource="#BciSession"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Study"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:about="#hasBciSession"/>
    </owl:inverseOf>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasGender">
    <rdfs:domain rdf:resource="#Subject"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Male</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Female</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/sex"/>
    <rdfs:comment xml:lang="en">Subject's gender.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#isBiomedicalResourceOf">
    <rdfs:domain rdf:resource="#BiomedicalResource"/>
    <rdfs:range rdf:resource="#BciRecord"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasBiomedicalResource"/>
    </owl:inverseOf>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasYearOfBirth">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment xml:lang="en">Subject's year of birth only.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Subject"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasHandedness">
    <rdfs:domain rdf:resource="#Subject"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/handedness"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >N/A</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Ambidextrous</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Left</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Right</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:comment xml:lang="en">Subject's dominantly used hand.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasOrganizationName">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciDevice"/>
          <owl:Class rdf:about="#Owner"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasHearing">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Subject"/>
          <owl:Class rdf:about="#RecordedSubjectAtSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">Subject's hearing.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >CorrectedToNormal</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Impaired</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Normal</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasWeight">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
    <rdfs:comment xml:lang="en">Subject's weight in kilograms (at the moment of the session). If not available, the default value would be the correspondant datatype property in "Subject".</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasName">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Place"/>
          <owl:Class rdf:about="#Owner"/>
          <owl:Class rdf:about="#Investigator"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">The name of the [entity].</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isSequentiallyLinkedTo">
    <rdfs:domain rdf:resource="#BiomedicalResource"/>
    <rdfs:range rdf:resource="#BiomedicalResource"/>
    <rdfs:comment xml:lang="en">A Biomedical resource (example: a *.bdf file) is sequentially linked to another Biomedical resource.  All the linked biomedical resources compose a BCI Record.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isBciRecordOf">
    <rdfs:range rdf:resource="#BciSession"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:about="#hasBciRecord"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#BciRecord"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasURL">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#AccessMethod.HTTP"/>
          <owl:Class rdf:about="#AccessMethod.FTP"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">The URI locator.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:subPropertyOf rdf:resource="#hasLocator"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#isUsedFor">
    <rdfs:comment xml:lang="en">Practical usage of the resource.  The resource is use as a...</rdfs:comment>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Annotation</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Raw data</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Event instance</rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Channel locations</rdf:first>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Descriptive metadata</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Experiment note</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Resource"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasModalityType">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">[ESS 1.0] Corresponds to the (/study/summary/recordedModalities/modality/name) node definition - name of a specific modality recorded in the study.</rdfs:comment>
    <rdfs:domain rdf:resource="#RecordedModality"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Motion Capture</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Video</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >EEG</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Audio</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasInSessionNumber">
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">[ESS 2.0] Each study may have multiple subjects. These subjects may or may not have labIds, but they are numbered for the sole purpose of distinction, arbitrarily and in the context of the session.
For example session 1 will have subjects with inSession numbers 1 and 2, while session 2 may also have subjects with InSession numbers 1 and 2 but they may refer to two other subjects.
This numbering is only to allow association between files and subjects in potential absence of subject labIds.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasStartTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment xml:lang="en">XSD DateTime of a specific starting-point of an INTERVAL (Time Ontology in OWL).
Definition: http://www.w3.org/TR/xmlschema11-2/#dateTime </rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Study"/>
          <owl:Class rdf:about="#BciRecord"/>
          <owl:Class rdf:about="#Resource"/>
          <owl:Class rdf:about="#BciSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasNumberOfUsedLEDs">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#RecordedModality"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasID">
    <rdfs:domain rdf:resource="#AccessMethod.MQTT"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">An string ID for an [entity].</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#isFromSubject">
    <rdfs:comment xml:lang="en">Information about the subject.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BiomedicalResource"/>
          <owl:Class rdf:about="#BciRecord"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Subject"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasDataSet"/>
    </owl:inverseOf>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasSamplingRate">
    <rdfs:comment xml:lang="en">Sampling rate of EEG recording (Hz).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciMeasurementProperty"/>
          <owl:Class rdf:about="#RecordedModality"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasNumberOfChannels">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Number of data channels.  In the case of "(EEG) BCI Record": of the used number of channels.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciMeasurementProperty"/>
          <owl:Class rdf:about="#EegBciRecord"/>
          <owl:Class rdf:about="#RecordedModality"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasHeight">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment xml:lang="en">Subject's height in centimeters (at the moment of the session). If not available, the default value would be the correspondant datatype property in "Subject".</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasVision">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Subject"/>
          <owl:Class rdf:about="#RecordedSubjectAtSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >CorrectedToNormal</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Impaired</rdf:first>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Normal</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Subject's vision</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasMIME-type">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >text/xml</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >text/plain</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >text/csv</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >application/pdf</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >application/atom+xml</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >application/xhtml+xml</rdf:first>
                        <rdf:rest rdf:parseType="Resource">
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >application/xml</rdf:first>
                          <rdf:rest rdf:parseType="Resource">
                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >application/rss+xml</rdf:first>
                            <rdf:rest rdf:parseType="Resource">
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >application/rdf+xml</rdf:first>
                            </rdf:rest>
                          </rdf:rest>
                        </rdf:rest>
                      </rdf:rest>
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >application/json</rdf:first>
                    </rdf:rest>
                  </rdf:rest>
                </rdf:rest>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >text/html</rdf:first>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:comment xml:lang="en">MIME type of the file.
Reference: https://en.wikipedia.org/wiki/Internet_media_type</rdfs:comment>
    <rdfs:domain rdf:resource="#Resource"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasAge">
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">Subject's age (in years) at the time of recording.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasEndTime">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Study"/>
          <owl:Class rdf:about="#BciRecord"/>
          <owl:Class rdf:about="#Resource"/>
          <owl:Class rdf:about="#BciSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="en">XSD DateTime of a specific end-point of an INTERVAL (Time Ontology in OWL).
 Definition: http://www.w3.org/TR/xmlschema11-2/#dateTime </rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#hasLocation">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:domain rdf:resource="#BciSession"/>
    <rdfs:range rdf:resource="#Place"/>
