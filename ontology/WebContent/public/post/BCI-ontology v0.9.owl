    <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
    <rdfs:comment xml:lang="en">A specific person (probably anonymous but possessing unique identity) with certain attributes on which the sessions are recorded. Same concept as "Patient".</rdfs:comment>
    <rdfs:label xml:lang="en">Subject</rdfs:label>
    <owl:disjointWith rdf:resource="#RecordedModality"/>
    <owl:disjointWith rdf:resource="#Owner"/>
    <owl:disjointWith rdf:resource="#BiomedicalResource"/>
    <owl:disjointWith rdf:resource="#BciSession"/>
    <owl:disjointWith rdf:resource="#Study"/>
    <owl:disjointWith rdf:resource="#BciRecord"/>
    <owl:disjointWith rdf:resource="#Place"/>
    <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
    <owl:disjointWith rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasYearOfBirth"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1900</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HED.Tactile">
    <rdfs:subClassOf rdf:resource="#HED.Stimulus"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="isResourceOf">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciRecord"/>
          <owl:Class rdf:about="#Study"/>
          <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
          <owl:Class rdf:about="#BiomedicalResource"/>
          <owl:Class rdf:about="#BciDevice"/>
          <owl:Class rdf:about="#Owner"/>
          <owl:Class rdf:about="#Place"/>
          <owl:Class rdf:about="#Subject"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Resource"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#hasResource"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementCapability">
    <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
    <rdfs:label xml:lang="en">hasMeasurementCapability</rdfs:label>
    <rdfs:subPropertyOf>
      <rdf:Property rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
    <rdfs:comment xml:lang="en">
        Relation from a Sensor to a MeasurementCapability describing the measurement properties of the sensor.
      </rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRecordedModality">
    <rdfs:range rdf:resource="#RecordedModality"/>
    <rdfs:comment xml:lang="en">[ESS 1.0] A BciRecord has a specific (single) defined RecordedModality.
[ESS 2.0] A BciRecord has a specific defined RecordedParameterSet, which groups various (multiple) RecordedModality. Therefore, a BciRecord can be associated with multiple RecordedModality definitions.
We keep this Object Property for faster graph traversal in queries.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciRecord"/>
          <owl:Class rdf:about="#RecordedParameterSet"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isUsedForGenerateMotionCaptureBciRecord">
    <rdfs:domain rdf:resource="#MotionCaptureBciDevice"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="isUsedForGenerateBciRecord"/>
    </rdfs:subPropertyOf>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isGeneratedByMotionCaptureBciDevice"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#MotionCaptureBciRecord"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isGeneratedByAudioBciDevice">
    <rdfs:domain rdf:resource="#AudioBciRecord"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isUsedForGenerateAudioBciRecord"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#AudioBciDevice"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="isGeneratedByBciDevice"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.oclc.org/NET/ssnx/ssn#hasMeasurementProperty">
    <rdfs:comment xml:lang="en">
        Relation from a MeasurementCapability to a MeasurementProperty. For 
        example, to an accuracy (see notes at MeasurementCapability).
      </rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementCapability"/>
    <rdfs:label xml:lang="en">hasMeasurementProperty</rdfs:label>
    <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#MeasurementProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isGeneratedByBciDevice">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="#isUsedForGenerateBciRecord"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#BciRecord"/>
    <rdfs:comment xml:lang="en">The device used.</rdfs:comment>
    <rdfs:range rdf:resource="#BciDevice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasOwner">
    <rdfs:range rdf:resource="#Owner"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Study"/>
          <owl:Class rdf:about="#Resource"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRecordedParameterSet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#RecordedParameterSet"/>
    <rdfs:comment xml:lang="en">[ESS 2.0] A BciRecord has a RecordedParameterSet.
This is found in
  &lt;recordingParameterSetLabel&gt; tag of &lt;recordingParameterSet&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#BciRecord"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isGeneratedByMotionCaptureBciDevice">
    <rdfs:domain rdf:resource="#MotionCaptureBciRecord"/>
    <rdfs:subPropertyOf rdf:resource="#isGeneratedByBciDevice"/>
    <owl:inverseOf rdf:resource="#isUsedForGenerateMotionCaptureBciRecord"/>
    <rdfs:range rdf:resource="#MotionCaptureBciDevice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasResource">
    <rdfs:range rdf:resource="#Resource"/>
    <owl:inverseOf rdf:resource="#isResourceOf"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciRecord"/>
          <owl:Class rdf:about="#Study"/>
          <owl:Class rdf:about="#BciDevice"/>
          <owl:Class rdf:about="#Owner"/>
          <owl:Class rdf:about="#Place"/>
          <owl:Class rdf:about="#Subject"/>
          <owl:Class rdf:about="#BciSession"/>
          <owl:Class rdf:about="#RecordedSubjectAtSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRecordedSpecsSubjectSession">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciSession"/>
          <owl:Class rdf:about="#Subject"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">The specifications of how a subject was recorded in a session.</rdfs:comment>
    <rdfs:range rdf:resource="#RecordedSubjectAtSession"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasChannelData">
    <rdfs:comment xml:lang="en">@XDF: The channeling information related to a specific EEG record.</rdfs:comment>
    <rdfs:range rdf:resource="#Channel"/>
    <rdfs:domain rdf:resource="#EegBciRecord"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isChannelDataOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasAccessMethod">
    <rdfs:label xml:lang="en">Data access method</rdfs:label>
    <rdfs:comment xml:lang="en">One or more mechanisms (protocols and identifiers) through which the data can be accessed.
Usually, it's a standard protocol, such as: MQTT, CoAP, http, etc.</rdfs:comment>
    <rdfs:range rdf:resource="#AccessMethod"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Resource"/>
          <owl:Class rdf:about="#BciDevice"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isGeneratedByVisualBciDevice">
    <rdfs:subPropertyOf rdf:resource="#isGeneratedByBciDevice"/>
    <rdfs:domain rdf:resource="#VideoBciRecord"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isUsedForGenerateVideoBciRecord"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#VisualBciDevice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isUsedForGenerateEegBciRecord">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isGeneratedByEegBciDevice"/>
    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#isUsedForGenerateBciRecord"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#EegBciDevice"/>
    <rdfs:range rdf:resource="#EegBciRecord"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isChannelDataOf">
    <rdfs:range rdf:resource="#EegBciRecord"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Channel"/>
    <owl:inverseOf rdf:resource="#hasChannelData"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isRelatedToResource">
    <rdfs:range rdf:resource="#Resource"/>
    <rdfs:domain rdf:resource="#BiomedicalResource"/>
    <rdfs:comment xml:lang="en">It states that [this entity] is logically related to multiple Resources (of diverse type).</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="specifiesBciDevice">
    <rdfs:comment xml:lang="en">[ESS 2.0] The "recordingParameterSet" node groups the information of multiple "modalities" and, also, "device" specs. Therefore, it implicitly defines that a "BciRecord" is generated by multiple "BciDevices", due of the "dataRecording" node definition in ESS.</rdfs:comment>
    <rdfs:domain rdf:resource="#RecordedParameterSet"/>
    <rdfs:range rdf:resource="#BciDevice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isUsedForGenerateBciRecord">
    <owl:inverseOf rdf:resource="#isGeneratedByBciDevice"/>
    <rdfs:domain rdf:resource="#BciDevice"/>
    <rdfs:range rdf:resource="#BciRecord"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isUsedForGenerateAudioBciRecord">
    <rdfs:domain rdf:resource="#AudioBciDevice"/>
    <rdfs:range rdf:resource="#AudioBciRecord"/>
    <rdfs:subPropertyOf rdf:resource="#isUsedForGenerateBciRecord"/>
    <owl:inverseOf rdf:resource="#isGeneratedByAudioBciDevice"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isGeneratedByEegBciDevice">
    <owl:inverseOf rdf:resource="#isUsedForGenerateEegBciRecord"/>
    <rdfs:range rdf:resource="#EegBciDevice"/>
    <rdfs:subPropertyOf rdf:resource="#isGeneratedByBciDevice"/>
    <rdfs:domain rdf:resource="#EegBciRecord"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isUsedForGenerateVideoBciRecord">
    <rdfs:range rdf:resource="#VideoBciRecord"/>
    <rdfs:domain rdf:resource="#VisualBciDevice"/>
    <owl:inverseOf rdf:resource="#isGeneratedByVisualBciDevice"/>
    <rdfs:subPropertyOf rdf:resource="#isUsedForGenerateBciRecord"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasReference.Label">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#EegBciRecord"/>
          <owl:Class rdf:about="#RecordedModality"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">@XDF: [Signal referencing scheme]
Name of the dedicated reference channel(s), if part of the measured channels (repeated if multiple).

[ESS 2.0] Labels of the reference channel or channels used during EEG or ECG recording. For example, if using 10-20 system and numerical average of both mastoids, use "A1, A2" for {referenceLabel} and "Mastoids" for {referenceLocation}. Note that there could be multiple labels.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasHardware.Coupling">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">Type of coupling used: Gel, Saline, Dry, Capacitive.</rdfs:comment>
    <rdfs:domain rdf:resource="#EegBciRecord"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Gel</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Capacitive</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Dry</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Saline</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSecurity">
    <rdfs:domain rdf:resource="#AccessMethod"/>
    <rdfs:comment xml:lang="en">The [ security ] of the "Access Method".</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSessionLabID">
    <rdfs:domain rdf:resource="#BciSession"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">Identifier of session used in original lab notes (if available, otherwise insert 'NA').
Reference: [ESS]</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasHardware.Surface">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">
      Type of the contact surface (e.g., Plate, Pins, Bristle, Pad)</rdfs:comment>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Plate</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Bristle</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Pad</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Pins</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#EegBciRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasMedicationAlcohol">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment xml:lang="en">Whether the subject has consumed alcohol within 24 hours before the recording (True=Yes or False=No).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#RecordedSubjectAtSession"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasEndChannel">
    <rdfs:comment xml:lang="en">The channel number in the recording where the modality block ends.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#RecordedModality"/>
          <owl:Class rdf:about="#BciMeasurementProperty"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/Person/height">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:label xml:lang="en">height (cm)</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasIdNumber">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#BciSession"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">Number that identifies individual entity.
