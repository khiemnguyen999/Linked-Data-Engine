  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasModalitySignalType">
    <rdfs:comment xml:lang="en">[ESS 2.0] The signal type: EEG (for Electroencephalogram), MoCap (for Motion Capture), Gaze (for gaze / eye tracking) parameters.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#RecordedModality"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
