    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AccessMethod -->

    <owl:Class rdf:about="&bci;AccessMethod">
        <rdfs:label xml:lang="en">Data access method</rdfs:label>
        <rdfs:comment xml:lang="en">One or more mechanisms (protocols and its identifiers) through which the resources (data) can be accessed.  Usually, it&apos;s a standard protocol, such as: MQTT, CoAP, http, etc.</rdfs:comment>
        <rdfs:comment xml:lang="en">comment 2: just for testing</rdfs:comment>
        
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AccessMethod.CoAP -->

    <owl:Class rdf:about="&bci;AccessMethod.CoAP">
        <rdfs:subClassOf rdf:resource="&bci;AccessMethod"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasType"/>
                <owl:hasValue rdf:datatype="&xsd;string">Archive</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.FTP"/>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.HTTP"/>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.MQTT"/>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AccessMethod.FTP -->

    <owl:Class rdf:about="&bci;AccessMethod.FTP">
        <rdfs:subClassOf rdf:resource="&bci;AccessMethod"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasType"/>
                <owl:hasValue rdf:datatype="&xsd;string">Archive</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.HTTP"/>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.MQTT"/>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AccessMethod.HTTP -->

    <owl:Class rdf:about="&bci;AccessMethod.HTTP">
        <rdfs:subClassOf rdf:resource="&bci;AccessMethod"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasType"/>
                <owl:hasValue rdf:datatype="&xsd;string">Archive</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&bci;AccessMethod.MQTT"/>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AccessMethod.MQTT -->

    <owl:Class rdf:about="&bci;AccessMethod.MQTT">
        <rdfs:label xml:lang="en">Message Queuing Telemetry Transport (MQTT)</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bci;AccessMethod"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasType"/>
                <owl:hasValue rdf:datatype="&xsd;string">Real-time</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Defines the parameters needed for a suitable MQTT access method.</rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AudioBciDevice -->

    <owl:Class rdf:about="&bci;AudioBciDevice">
