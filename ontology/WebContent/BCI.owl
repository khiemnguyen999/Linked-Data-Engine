<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY dbp "http://dbpedia.org/ontology/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY ssn "http://purl.oclc.org/NET/ssnx/ssn#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY bci "http://bci.pet.cs.nctu.edu.tw/ontology#" >
    <!ENTITY dul "http://www.loa-cnr.it/ontologies/DUL.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://bci.pet.cs.nctu.edu.tw/ontology#"
     xml:base="http://bci.pet.cs.nctu.edu.tw/ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:bci="http://bci.pet.cs.nctu.edu.tw/ontology#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:ssn="http://purl.oclc.org/NET/ssnx/ssn#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:dul="http://www.loa-cnr.it/ontologies/DUL.owl#"
     xmlns:dbp="http://dbpedia.org/ontology/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://bci.pet.cs.nctu.edu.tw/ontology">
        <rdfs:label rdf:datatype="&rdfs;Literal">BCI ontology</rdfs:label>
        <owl:versionInfo rdf:datatype="&rdfs;Literal">0.11</owl:versionInfo>
        <dc:creator rdf:datatype="&rdfs;Literal">Sergio Jose Rodriguez Mendez. PET Lab, Computer Science College, NCTU, Taiwan.</dc:creator>
        <rdfs:comment rdf:datatype="&rdfs;Literal">[1] This ontology describes the framework of core concepts to capture the specialized meta-data vocabulary set for multimodal &quot;Brain Computing Interface&quot; (BCI) activities.</rdfs:comment>
        <rdfs:comment rdf:datatype="&rdfs;Literal">[2] This ontology is developed by the PET Lab at the Computer Science College of NCTU, Taiwan (R.O.C). The concepts and structure of the ontology depict a basic/initial meta-data model for BCI activities.</rdfs:comment>
        <rdfs:comment rdf:datatype="&rdfs;Literal">[3] Any feedback is welcome. Please mail it to srodriguez@pet.cs.nctu.edu.tw</rdfs:comment>
        <rdfs:comment rdf:datatype="&rdfs;Literal">[PREFIX] When used in XML documents the suggested prefix is &quot;bci&quot;</rdfs:comment>
        <dc:title>Brain Computing Interface (BCI) Ontology</dc:title>
        <dc:rights>Copyright 2014 - 2015 PET Lab, Computer Science College, NCTU, Taiwan.</dc:rights>
        <dct:created>2014-03-27</dct:created>
        <dct:modified>2015-01-20</dct:modified>
        <dc:identifier>http://bci.pet.cs.nctu.edu.tw/ontology#</dc:identifier>
        <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl"/>
		<owl:imports rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
        <owl:imports rdf:resource="http://dbpedia.org/ontology/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    


    <!-- http://purl.org/dc/elements/1.1/identifier -->

    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/source -->

    <owl:AnnotationProperty rdf:about="&dc;source"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="&dct;created"/>
    


    <!-- http://purl.org/dc/terms/modified -->

    <owl:AnnotationProperty rdf:about="&dct;modified"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#describesStimulusIn -->

    <owl:ObjectProperty rdf:about="&bci;describesStimulusIn">
        <rdfs:range rdf:resource="&bci;LogicalDataPointer"/>
        <rdfs:domain rdf:resource="&ssn;Stimulus"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasAccessMethod -->

    <owl:ObjectProperty rdf:about="&bci;hasAccessMethod">
        <rdfs:label xml:lang="en">Data access method</rdfs:label>
        <rdfs:comment xml:lang="en">One or more mechanisms (protocols and identifiers) through which the data can be accessed.
Usually, it&apos;s a standard protocol, such as: MQTT, CoAP, http, etc.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;AccessMethod"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciDevice"/>
                    <rdf:Description rdf:about="&bci;Resource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasBciRecord -->

    <owl:ObjectProperty rdf:about="&bci;hasBciRecord">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">Information about each individual &quot;BCI Record&quot; that is part of a &quot;BCI Session&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;BciRecord"/>
        <rdfs:domain rdf:resource="&bci;BciSession"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasBciSession -->

    <owl:ObjectProperty rdf:about="&bci;hasBciSession">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">
      Information about each individual &quot;Session&quot; that is part of a &quot;Collection&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;BciSession"/>
        <rdfs:domain rdf:resource="&bci;Study"/>
        <owl:inverseOf rdf:resource="&bci;isBciSessionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasBiomedicalResource -->

    <owl:ObjectProperty rdf:about="&bci;hasBiomedicalResource">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">Information about biomedical recorded files or streams (EEG, EMG, EOG, etc). Each individual represents a raw BCI recorded file/stream of a specific format.</rdfs:comment>
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <rdfs:range rdf:resource="&bci;BiomedicalResource"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <rdfs:range rdf:resource="&bci;Channel"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasCondition -->

    <owl:ObjectProperty rdf:about="&bci;hasCondition">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Information about the condition of the specified event code.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;BciCondition"/>
        <rdfs:domain rdf:resource="&bci;EventCode"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasDataSet -->

    <owl:ObjectProperty rdf:about="&bci;hasDataSet">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;Subject"/>
        <owl:inverseOf rdf:resource="&bci;isFromSubject"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciRecord"/>
                    <rdf:Description rdf:about="&bci;BiomedicalResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasEegChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasEegChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&rdfs;Literal">@XDF: The channeling information related to a specific EEG record.</rdfs:comment>
        <rdfs:domain rdf:resource="&bci;EegBciRecord"/>
        <rdfs:range rdf:resource="&bci;EegChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;hasChannelData"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasEventCode -->

    <owl:ObjectProperty rdf:about="&bci;hasEventCode">
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <rdfs:range rdf:resource="&bci;EventCode"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasEyeGazeChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasEyeGazeChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&rdfs;Literal">@XDF: The channeling information related to a specific Eye-Gaze record.</rdfs:comment>
        <rdfs:domain rdf:resource="&bci;EyeGazeBciRecord"/>
        <rdfs:range rdf:resource="&bci;EyeGazeChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;hasChannelData"/>
        <owl:inverseOf rdf:resource="&bci;isEyeGazeChannelDataOf"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasHandGestureChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasHandGestureChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&rdfs;Literal">The channeling information related to a specific Hand-Gesture record.</rdfs:comment>
        <rdfs:domain rdf:resource="&bci;HandGestureBciRecord"/>
        <rdfs:range rdf:resource="&bci;HandGestureChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;hasChannelData"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasKeyboardHitChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasKeyboardHitChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;KeyboardHitBciRecord"/>
        <rdfs:range rdf:resource="&bci;KeyboardHitChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;hasChannelData"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasMember -->

    <owl:ObjectProperty rdf:about="&bci;hasMember">
        <rdfs:domain rdf:resource="&bci;Bundle"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciRecord"/>
                    <rdf:Description rdf:about="&bci;BciSession"/>
                    <rdf:Description rdf:about="&bci;Study"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasMouseClickChannelData -->

    <owl:ObjectProperty rdf:about="&bci;hasMouseClickChannelData">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;MouseClickBciRecord"/>
        <rdfs:range rdf:resource="&bci;MouseClickChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;hasChannelData"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasOwner -->

    <owl:ObjectProperty rdf:about="&bci;hasOwner">
        <rdfs:range rdf:resource="&bci;Owner"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;Bundle"/>
                    <rdf:Description rdf:about="&bci;LogicalDataPointer"/>
                    <rdf:Description rdf:about="&bci;Resource"/>
                    <rdf:Description rdf:about="&bci;Study"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasRecordedModality -->

    <owl:ObjectProperty rdf:about="&bci;hasRecordedModality">
        <rdfs:comment xml:lang="en">[ESS 1.0] A BciRecord has a specific (single) defined RecordedModality.
[ESS 2.0] A BciRecord has a specific defined RecordedParameterSet, which groups various (multiple) RecordedModality. Therefore, a BciRecord can be associated with multiple RecordedModality definitions.
We keep this Object Property for faster graph traversal in queries.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;RecordedModality"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciRecord"/>
                    <rdf:Description rdf:about="&bci;RecordedParameterSet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasRecordedParameterSet -->

    <owl:ObjectProperty rdf:about="&bci;hasRecordedParameterSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">[ESS 2.0] A BciRecord has a RecordedParameterSet.
This is found in
  &lt;recordingParameterSetLabel&gt; tag of &lt;recordingParameterSet&gt;</rdfs:comment>
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <rdfs:range rdf:resource="&bci;RecordedParameterSet"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasRecordedSpecsSubjectSession -->

    <owl:ObjectProperty rdf:about="&bci;hasRecordedSpecsSubjectSession">
        <rdfs:comment xml:lang="en">The specifications of how a subject was recorded in a session.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;RecordedSubjectAtSession"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciSession"/>
                    <rdf:Description rdf:about="&bci;Subject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasResource -->

    <owl:ObjectProperty rdf:about="&bci;hasResource">
        <rdfs:range rdf:resource="&bci;Resource"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciCondition"/>
                    <rdf:Description rdf:about="&bci;BciDevice"/>
                    <rdf:Description rdf:about="&bci;BciRecord"/>
                    <rdf:Description rdf:about="&bci;BciSession"/>
                    <rdf:Description rdf:about="&bci;Bundle"/>
                    <rdf:Description rdf:about="&bci;Owner"/>
                    <rdf:Description rdf:about="&bci;Place"/>
                    <rdf:Description rdf:about="&bci;RecordedSubjectAtSession"/>
                    <rdf:Description rdf:about="&bci;Study"/>
                    <rdf:Description rdf:about="&bci;Subject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasResourceDataBlock -->

    <owl:ObjectProperty rdf:about="&bci;hasResourceDataBlock">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">hasResourceDataBlock</rdfs:label>
        <rdfs:domain rdf:resource="&bci;Resource"/>
        <rdfs:range rdf:resource="&bci;ResourceDataBlock"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#hasStimulusTag -->

    <owl:ObjectProperty rdf:about="&bci;hasStimulusTag">
        <rdfs:domain rdf:resource="&bci;LogicalDataPointer"/>
        <owl:inverseOf rdf:resource="&bci;describesStimulusIn"/>
        <rdfs:range rdf:resource="&ssn;Stimulus"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isBciRecordOf -->

    <owl:ObjectProperty rdf:about="&bci;isBciRecordOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <rdfs:range rdf:resource="&bci;BciSession"/>
        <owl:inverseOf rdf:resource="&bci;hasBciRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isBciSessionOf -->

    <owl:ObjectProperty rdf:about="&bci;isBciSessionOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&bci;BciSession"/>
        <rdfs:range rdf:resource="&bci;Study"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isBiomedicalResourceOf -->

    <owl:ObjectProperty rdf:about="&bci;isBiomedicalResourceOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&bci;BciRecord"/>
        <rdfs:domain rdf:resource="&bci;BiomedicalResource"/>
        <owl:inverseOf rdf:resource="&bci;hasBiomedicalResource"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isChannelDataOf -->

    <owl:ObjectProperty rdf:about="&bci;isChannelDataOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&bci;BciRecord"/>
        <rdfs:domain rdf:resource="&bci;Channel"/>
        <owl:inverseOf rdf:resource="&bci;hasChannelData"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isEegChannelDataOf -->

    <owl:ObjectProperty rdf:about="&bci;isEegChannelDataOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&bci;EegBciRecord"/>
        <rdfs:domain rdf:resource="&bci;EegChannel"/>
        <owl:inverseOf rdf:resource="&bci;hasEegChannelData"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isChannelDataOf"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isEyeGazeChannelDataOf -->

    <owl:ObjectProperty rdf:about="&bci;isEyeGazeChannelDataOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&bci;EyeGazeBciRecord"/>
        <rdfs:domain rdf:resource="&bci;EyeGazeChannel"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isChannelDataOf"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isFromSubject -->

    <owl:ObjectProperty rdf:about="&bci;isFromSubject">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Information about the subject.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;Subject"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&bci;BciRecord"/>
                    <rdf:Description rdf:about="&bci;BiomedicalResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByAudioBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByAudioBciDevice">
        <rdfs:range rdf:resource="&bci;AudioBciDevice"/>
        <rdfs:domain rdf:resource="&bci;AudioBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByBciDevice">
        <rdfs:comment xml:lang="en">The device used.</rdfs:comment>
        <rdfs:range rdf:resource="&bci;BciDevice"/>
        <rdfs:domain rdf:resource="&bci;BciRecord"/>
        <owl:inverseOf rdf:resource="&bci;isUsedForGenerateBciRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByEegBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByEegBciDevice">
        <rdfs:range rdf:resource="&bci;EegBciDevice"/>
        <rdfs:domain rdf:resource="&bci;EegBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
        <owl:inverseOf rdf:resource="&bci;isUsedForGenerateEegBciRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByEyeGazeBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByEyeGazeBciDevice">
        <rdfs:range rdf:resource="&bci;EyeGazeBciDevice"/>
        <rdfs:domain rdf:resource="&bci;EyeGazeBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByHandGestureBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByHandGestureBciDevice">
        <rdfs:range rdf:resource="&bci;HandGestureBciDevice"/>
        <rdfs:domain rdf:resource="&bci;HandGestureBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByKeyboardHitBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByKeyboardHitBciDevice">
        <rdfs:range rdf:resource="&bci;KeyboardHitBciDevice"/>
        <rdfs:domain rdf:resource="&bci;KeyboardHitBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByMotionCaptureBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByMotionCaptureBciDevice">
        <rdfs:range rdf:resource="&bci;MotionCaptureBciDevice"/>
        <rdfs:domain rdf:resource="&bci;MotionCaptureBciRecord"/>
        <rdfs:subPropertyOf rdf:resource="&bci;isGeneratedByBciDevice"/>
        <owl:inverseOf rdf:resource="&bci;isUsedForGenerateMotionCaptureBciRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#isGeneratedByMouseClickBciDevice -->

    <owl:ObjectProperty rdf:about="&bci;isGeneratedByMouseClickBciDevice">
        <rdfs:range rdf:resource="&bci;MouseClickBciDevice"/>
