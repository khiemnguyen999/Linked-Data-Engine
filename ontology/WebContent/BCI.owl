        <rdfs:subClassOf rdf:resource="&bci;BciDevice"/>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#AudioBciRecord -->

    <owl:Class rdf:about="&bci;AudioBciRecord">
        <rdfs:subClassOf rdf:resource="&bci;BciRecord"/>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BciCondition -->

    <owl:Class rdf:about="&bci;BciCondition">
        <rdfs:label xml:lang="en">BCI Condition</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;Condition"/>
        <owl:disjointWith rdf:resource="&bci;BciMeasurementProperty"/>
        <rdfs:comment xml:lang="en"></rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BciDevice -->

    <owl:Class rdf:about="&bci;BciDevice">
        <rdfs:label xml:lang="en">BCI Device</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;SensingDevice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasAccessMethod"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ssn;hasMeasurementCapability"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"></rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BciMeasurementProperty -->

    <owl:Class rdf:about="&bci;BciMeasurementProperty">
        <rdfs:label xml:lang="en">BCI Measurement Property</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;MeasurementProperty"/>
        <rdfs:comment xml:lang="en"></rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BciRecord -->

    <owl:Class rdf:about="&bci;BciRecord">
        <rdfs:label xml:lang="en">BCI Record</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;ObservationValue"/>
        <owl:disjointWith rdf:resource="&bci;BciSession"/>
        <owl:disjointWith rdf:resource="&bci;BiomedicalResource"/>
        <owl:disjointWith rdf:resource="&bci;EventCode"/>
        <owl:disjointWith rdf:resource="&bci;Feature"/>
        <owl:disjointWith rdf:resource="&bci;Owner"/>
        <owl:disjointWith rdf:resource="&bci;Place"/>
        <owl:disjointWith rdf:resource="&bci;RecordedModality"/>
        <owl:disjointWith rdf:resource="&bci;Study"/>
        <owl:disjointWith rdf:resource="&bci;Subject"/>
        <owl:disjointWith rdf:resource="&ssn;Observation"/>
        <owl:disjointWith rdf:resource="&ssn;Property"/>
        <owl:disjointWith rdf:resource="&ssn;Sensor"/>
        <owl:disjointWith rdf:resource="&dul;Event"/>
        <rdfs:comment xml:lang="en">[ESS 1.0] Corresponds to the &quot;eegRecordings&quot; node: a specific collection of raw BCI data collected from a subject in a specific session.
[ESS 2.0] Corresponds to the &quot;dataRecordings&quot; node: information about EEG (or other data) recordings.</rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BciSession -->

    <owl:Class rdf:about="&bci;BciSession">
        <rdfs:label xml:lang="en">BCI Session</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ssn;Observation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasResource"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;hasBciRecord"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&bci;BiomedicalResource"/>
        <owl:disjointWith rdf:resource="&bci;EventCode"/>
        <owl:disjointWith rdf:resource="&bci;Feature"/>
        <owl:disjointWith rdf:resource="&bci;Owner"/>
        <owl:disjointWith rdf:resource="&bci;Place"/>
        <owl:disjointWith rdf:resource="&bci;RecordedModality"/>
        <owl:disjointWith rdf:resource="&bci;Study"/>
        <owl:disjointWith rdf:resource="&bci;Subject"/>
        <owl:disjointWith rdf:resource="&ssn;Observation"/>
        <owl:disjointWith rdf:resource="&ssn;Property"/>
        <owl:disjointWith rdf:resource="&ssn;Sensor"/>
        <owl:disjointWith rdf:resource="&dul;Event"/>
        <rdfs:comment xml:lang="en">A BCI session is a type of ssn:Observation:
      - It&apos;s a Situation for BCI purposes.
      - A Sensing method it&apos;s used to estimate or calculate a value of different Property.

      [Definition]: a collection of multi-modal biomedical data files recorded from a certain Subject in a specific Event.</rdfs:comment>
    </owl:Class>
    


    <!-- http://bci.pet.cs.nctu.edu.tw/ontology#BiomedicalResource -->

    <owl:Class rdf:about="&bci;BiomedicalResource">
        <rdfs:label xml:lang="en">Biomedical resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bci;Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;isBiomedicalResourceOf"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;isUsedFor"/>
                <owl:hasValue rdf:datatype="&xsd;string">Raw data</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&bci;isFromSubject"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&bci;EventCode"/>
        <owl:disjointWith rdf:resource="&bci;Feature"/>
        <owl:disjointWith rdf:resource="&bci;Owner"/>
        <owl:disjointWith rdf:resource="&bci;Place"/>
        <owl:disjointWith rdf:resource="&bci;RecordedModality"/>
        <owl:disjointWith rdf:resource="&bci;Study"/>
        <owl:disjointWith rdf:resource="&bci;Subject"/>
