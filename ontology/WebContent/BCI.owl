                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">DiameterX</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">DiameterY</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first rdf:datatype="&xsd;string">DirectionX</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                <rdf:first rdf:datatype="&xsd;string">DirectionY</rdf:first>
                                                                <rdf:rest>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                        <rdf:first rdf:datatype="&xsd;string">DirectionZ</rdf:first>
                                                                        <rdf:rest>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                                <rdf:first rdf:datatype="&xsd;string">FrameNumber</rdf:first>
                                                                                <rdf:rest>
                                                                                    <rdf:Description>
                                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                                        <rdf:first rdf:datatype="&xsd;string">HeadX</rdf:first>
                                                                                        <rdf:rest>
                                                                                            <rdf:Description>
                                                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                                                <rdf:first rdf:datatype="&xsd;string">HeadY</rdf:first>
                                                                                                <rdf:rest>
                                                                                                    <rdf:Description>
                                                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                                                        <rdf:first rdf:datatype="&xsd;string">HeadZ</rdf:first>
                                                                                                        <rdf:rest>
                                                                                                            <rdf:Description>
                                                                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                                                                <rdf:first rdf:datatype="&xsd;string">IntersectionX</rdf:first>
                                                                                                                <rdf:rest>
                                                                                                                    <rdf:Description>
                                                                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                                                                        <rdf:first rdf:datatype="&xsd;string">IntersectionY</rdf:first>
                                                                                                                        <rdf:rest>
                                                                                                                            <rdf:Description>
                                                                                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                                                                                <rdf:first rdf:datatype="&xsd;string">IntersectionZ</rdf:first>
                                                                                                                                <rdf:rest>
                                                                                                                                    <rdf:Description>
                                                                                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                                                                                        <rdf:first rdf:datatype="&xsd;string">ObjectId</rdf:first>
                                                                                                                                        <rdf:rest>
